<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>


<h1>Sign up here!</h1>
<%= form_with model: @user do |u| %>
<%= u.label :email %> 
<%= u.text_field :email %> 
<%= u.label :username %> 
<%= u.text_field :username %> 
<%= u.label :password %> 
<%= u.password_field :password %> 
<%= f.submit 'Create Profile' %>
<% end %>
